<Window x:Class="OOSD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OOSD"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="addPerson" Content="Add Individual" HorizontalAlignment="Left" Margin="66,65,0,0" VerticalAlignment="Top" Width="167" Height="74" Click="addPerson_Click"/>
        <Button x:Name="addLocation" Content="Add Location" HorizontalAlignment="Left" Margin="545,65,0,0" VerticalAlignment="Top" Width="167" Height="74" Click="addLocation_Click"/>
        <Button x:Name="database" Content="Database" HorizontalAlignment="Left" Margin="653,354,0,0" VerticalAlignment="Top" Width="116" Height="56" Click="database_Click"/>
        <Button x:Name="recordVisit" Content="Record Visit" HorizontalAlignment="Left" Margin="545,216,0,0" VerticalAlignment="Top" Width="167" Height="74" Click="recordVisit_Click"/>
        <Button x:Name="recordContact" Content="Record Contact" HorizontalAlignment="Left" Margin="66,216,0,0" VerticalAlignment="Top" Width="167" Height="74" Click="recordContact_Click"/>

    </Grid>
</Window>
